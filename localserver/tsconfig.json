    "target": "ES2017",
    "moduleResolution": "node",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "*": ["../*"],
      "@app/*": ["../app/*"],
      "@lambda-app/*": ["../lambda/app/src/*"]
    }
  }
}
